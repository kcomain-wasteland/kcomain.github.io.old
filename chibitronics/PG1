}
function dsth() {
    scope.writeLine("Hello!")
    scope.writeNumber(0)
    scope.writeLine("Hello!")
    scope.writeNumber(0)
}
sensing.onPinEvent(DigitalPin.D1, PinEvent.Pressed, function () {
    pins.digitalWrite(DigitalPin.D0, 0)
    scope.writeLine("Hello!")
    loops.pause(100)
    scope.writeNumber(1)
    loops.pause(100)
    scope.writeLine("2")
    loops.pause(100)
    scope.writeNumber(3)
    loops.pause(100)
    scope.writeLine("4")
    loops.pause(100)
    scope.writeLine("5")
    loops.pause(100)
    scope.writeLine("6")
    loops.pause(100)
    scope.writeNumber(7)
    loops.pause(100)
    scope.writeLine("8")
    loops.pause(100)
    scope.writeNumber(9)
    scope.writeLine("Hello!")
    loops.pause(100)
    scope.writeNumber(1)
    loops.pause(100)
    scope.writeLine("2")
    loops.pause(100)
    scope.writeNumber(3)
    loops.pause(100)
    scope.writeLine("4")
    loops.pause(100)
    scope.writeLine("5")
    loops.pause(100)
    scope.writeLine("6")
    loops.pause(100)
    scope.writeNumber(7)
    loops.pause(100)
    scope.writeLine("8")
    loops.pause(100)
    scope.writeNumber(9)
})
sensing.onPinEvent(DigitalPin.D0, PinEvent.Pressed, function () {
    pins.digitalWrite(DigitalPin.D0, 1)
})
aaaa()
